# Rails *****************************
snippet sff
	<%= simple_form_for(${1:resource}, :as => ${2:resource_name}, :url => session_path($2), :html => { :class => '${3:form-horizontal}'}) do |${4:f}| %>
	<% end %>
snippet rt.
	t('${1:layouts}.${2:application/placehold/form}.${3:email/password/btn_signin}')
snippet ri
	<%= f.input :${1:email/password/remember_me}, ${2::label => false}, ${3::placeholder => t('layouts.placeholder.}$1')} %>
snippet rip
