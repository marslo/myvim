snippet rl
	<%= f.label ${1:t('layouts.form.email')}, ${2::class => 'control-label'} %>
snippet rl.
	<%= f.label t('${1:layouts}.${2:form}.${3:email}'), :class => ${4:'control-label'} %>
snippet rc
	<% ${1} %>
snippet rce
	<%= ${1} %>
snippet re
	<%= ${1} %>
snippet end
	<% end %>
snippet else
	<% else %>
snippet if
	<% if ${1:condition} %>
	  ${2: <# <% code %>}
	<% end %>
snippet ifel
	<% if ${1:condition} %>
	  ${2: <# <% code %>}
	<% else %>
	  ${3: <# <% code %>}
	<% end %>
snippet for
